<%- include('layouts/header.ejs') %>
<%- include('layouts/admin-nav.ejs') %>
<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <div class="container-full">
      <!-- Content Header (Page header) -->	  
      <div class="content-header">
          <div class="d-flex align-items-center">
              <div class="me-auto">
                  <h4 class="page-title">Edit Sector</h4>
                  <div class="d-inline-block align-items-center">
                      <nav>
                          <ol class="breadcrumb">
                              <li class="breadcrumb-item"><a href="#"><i class="mdi mdi-home-outline"></i></a></li>
                              <li class="breadcrumb-item" aria-current="page">Edit Sector</li>
                          </ol>
                      </nav>
                  </div>
              </div>
              
          </div>
      </div>

      <!-- Main content -->
      <section class="content">
          <div class="row">
            <div class="col-lg-12 col-12">
                <div class="box">
                  <div class="box-header with-border">
                    <h4 class="box-title">Edit Sector</h4>
                  </div>
                  <!-- /.box-header -->
                  <form class="form"  onsubmit="return false;" id="form">
                      <div class="box-body">
                        
                          <div class="row">
                            <div class="alert alert-success mt-2" role="alert" id="success" style="display: none;">
                       
                            </div>
                            <div class="alert alert-danger mt-2" role="alert" id="failed" style="display: none;">
                             
                            </div>
                            <div class="col-md-4">
                                <div class="form-group">
                                  <label class="form-label">Sector Name</label>
                                  <input type="text" class="form-control" placeholder="Sector Name" id="sector_name" name="sector_name" value="<%= sector_details.sector_name %>" required>
                                </div>
                              </div>
                             
                              <div class="col-md-4">
                                  <div class="form-group">
                                    <label class="form-label">Email</label>
                                    <input type="text" class="form-control" placeholder="Email" id="email" name="email" value="<%= sector_details.email %>" required>
                                  </div>
                              </div>
                          </div>
                          <input type="hidden" class="md-input label-fixed" id="id" value="<%= sector_details.id %>"/>
                      </div>
                      <!-- /.box-body -->
                      <div class="box-footer">
                         
                          <button type="submit" class="btn btn-primary">
                            <i class="ti-save-alt"></i> Save
                          </button>
                      </div>  
                  </form>
                </div>
                <!-- /.box -->			
          </div>
          </div>
      </section>

    
    </div>
</div>
</div>
<script src="../custom-js/edit-sector.js"></script>
<%- include('layouts/footer.ejs') %>